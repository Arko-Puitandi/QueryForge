# Environment Variables
# Copy this file to server/.env and fill in your values

# Server Configuration
PORT=3001
NODE_ENV=development

# Required: Gemini API Key (at least one)
GEMINI_API_KEY=your_primary_gemini_api_key_here

# Optional: Additional API Keys (for rotation when rate limited)
GEMINI_API_KEY_1=backup_key_1
GEMINI_API_KEY_2=backup_key_2
GEMINI_API_KEY_3=backup_key_3
GEMINI_API_KEY_4=backup_key_4

# Database
DATABASE_PATH=./data/history.db

# Logging
LOG_LEVEL=debug

# CORS
ALLOWED_ORIGINS=http://localhost:5173